digraph G {
    ranksep=0.5;
    nodesep=0.5;
    rankdir=LR;
    node [shape="circle", fontsize="16"];
    fontsize="10";
    compound=true;

    ia [shape=point, style=invis];
    d [shape=doublecircle];

    ia -> a;
    a -> d [label="0"];
    a -> a [label="1"];
    d -> sink [label="1"];
    d -> d [label="0"];
    sink -> sink [label="0, 1"];
}
