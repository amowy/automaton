digraph G {
    ranksep=0.5;
    nodesep=0.5;
    rankdir=LR;
    node [shape="circle", fontsize="16"];
    fontsize="10";
    compound=true;

    ia [shape=point, style=invis];
    c [shape=doublecircle];

    ia -> a;
    c -> c [label="0"];
    b -> b [label="1"];
    a -> b [label="1"];
    b -> c [label="0"];
}
